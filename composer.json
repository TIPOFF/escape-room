{
    "name": "tipoff/escape-room",
    "description": "Laravel package for escape room models and logic",
    "keywords": [
        "tipoff",
        "escape-room"
    ],
    "homepage": "https://github.com/tipoff/escape-room",
    "license": "MIT",
    "authors": [
        {
            "name": "Tipoff",
            "email": "packages@tipoff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "drewroberts/media": "^3.0.5",
        "nesbot/carbon": "^2.44",
        "tipoff/authorization": "^1.0.1",
        "tipoff/support": "^1.2.7"
    },
    "require-dev": {
        "tipoff/test-support": "^1.0.3"
    },
    "autoload": {
        "psr-4": {
            "Tipoff\\EscapeRoom\\": "src",
            "Tipoff\\EscapeRoom\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tipoff\\EscapeRoom\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tipoff\\EscapeRoom\\EscapeRoomServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
